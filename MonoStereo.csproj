<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>MonoStereo</Title>
    <Authors>Nycro</Authors>
    <Company>$(AssemblyName)</Company>
    <Description>A NAudio based audio engine for MonoGame.</Description>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Vorbis\NVorbis\NVorbis.csproj" />
    <None Include="Vorbis\OggVorbisEncoder\OggVorbisEncoder.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="MonoGame.Framework.Content.Pipeline" Version="3.8.1.303">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="MonoGame.Framework.DesktopGL" Version="3.8.1.303">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="NAudio" Version="2.2.1" />
    <PackageReference Include="z440.atl.core" Version="6.1.0" />
  </ItemGroup>
</Project>